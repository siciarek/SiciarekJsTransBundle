{
    "name": "siciarek/jstrans-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 javascript i18n support",
    "keywords": ["symfony", "javascript", "translation", "i18n"],
    "homepage": "http://siciarek.pl",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacek Siciarek",
            "email": "siciarek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.0"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "phpunit/phpunit": "3.7.*",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0"
    },
    "autoload": {
        "psr-0": { "Siciarek\\JsTransBundle": "" }
    },
    "target-dir": "Siciarek/JsTransBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
