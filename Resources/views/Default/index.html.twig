{% if app.environment == 'prod' %}
    <script src="{{ asset('bundles/siciarekjstrans/js/trans.js') }}"></script>
    <script src="{{ asset('bundles/siciarekjstrans/js/qunit/js-trans-test.js') }}"></script>
{% else %}
    {% javascripts '@SiciarekJsTransBundle/Resources/public/js/trans.js' output='js/trans.js' %}
    <script src="{{ asset_url }}"></script>{% endjavascripts %}
    {% javascripts '@SiciarekJsTransBundle/Resources/public/js/qunit/js-trans-test.js' output='js/qunit/js-trans-test.js' %}
    <script src="{{ asset_url }}"></script>{% endjavascripts %}
{% endif %}

